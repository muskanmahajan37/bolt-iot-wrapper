{
  "name": "bolt-iot-wrapper",
  "version": "1.0.0",
  "description": "bolt iot api wrappers",
  "main": "dist/exports.amd.js",
  "scripts": {
    "build": "rimraf dist && npm run buildBrowser && npm run buildNode",
    "buildBrowser": "parcel build src/exports.ts -o exports.cjs.js",
    "buildNode": "parcel build src/exports.ts --target node -o exports.amd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pranaydutta89/bolt-iot-wrapper.git"
  },
  "keywords": [
    "bolt",
    "iot",
    "api",
    "wrapper"
  ],
  "author": "Pranay Dutta",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pranaydutta89/bolt-iot-wrapper/issues"
  },
  "homepage": "https://github.com/pranaydutta89/bolt-iot-wrapper#readme",
  "devDependencies": {
    "node-fetch": "2.3.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "3.3.3333",
    "@types/node-fetch": "2.1.6",
    "rimraf": "2.6.3",
    "tslint": "5.14.0",
    "parcel": "1.12.2"
  }
}